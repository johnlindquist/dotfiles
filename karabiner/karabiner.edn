             :i-mode {:key :l}; "i" in colemak
             :period-mode {:key :period}; "i" in colemak
             :semicolon-mode {:key :semicolon}; "i" in colemak
             :l-mode {:key :u}; "l" in colemak
             :kit-mode {:key :n}; "k" in colemak
             :open-mode {:key :semicolon}; "o" in colemak
             :snippet-mode {:key :quote}
